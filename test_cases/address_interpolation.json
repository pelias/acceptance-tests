{
  "name": "address interpolation",
  "endpoint": "search",
  "priorityThresh": 1,
  "distanceThresh": 50,
  "tests": [
    {
      "id": 1,
      "user": "missinglink",
      "status": "pass",
      "in": {
        "text": "475f west 26th street, new york"
      },
      "expected": {
        "properties": [
          {
            "layer": "address",
            "source": "mixed",
            "housenumber": "475",
            "street": "West 26th Street",
            "match_type": "interpolated",
            "accuracy": "point"
          }
        ],
        "coordinates": [
          [ -74.0026336, 40.7495275 ]
        ]
      }
    },
    {
      "id": "1a",
      "user": "missinglink",
      "status": "pass",
      "description": "fails when specifying size param",
      "issue": "https://github.com/pelias/pelias/issues/562",
      "in": {
        "text": "475f west 26th street, new york",
        "size": 1
      },
      "expected": {
        "properties": [
          {
            "layer": "address",
            "source": "mixed",
            "housenumber": "475",
            "street": "West 26th Street",
            "match_type": "interpolated",
            "accuracy": "point"
          }
        ],
        "coordinates": [
          [ -74.0026336, 40.7495275 ]
        ]
      }
    },
    {
      "id": 2,
      "status": "pass",
      "user": "missinglink",
